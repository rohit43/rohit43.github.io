  author       = {Reznikov, Leah R and Norris, Michael H and Vashisht, Rohit and
                  Bluhm, Andrew P and Li, Danmeng and Liao, Yan-Shin J and
                  Brown, Ashley and Butte, Atul J and Ostrov, David A},
  journal = {Biochemical and biophysical research communications},
  publisher    = {Elsevier BV},
  volume       = {538},
  pages        = {173--179},
